{
    "name": "vynchronize",
    "version": "1.0.0",
    "description": "Video synchronization app",
    "main": "server.js",
    "scripts": {
        "test": "jshint test.js",
        "compile-apputilsupdatejs": "babel app\\utils\\update.js -o app\\utils\\update.js --require babel-polyfill --presets es2015-without-strict",
        "compile-assetsmainjs": "babel assets\\main.js -o assets\\main.js --require babel-polyfill --presets es2015-without-strict",
        "compile-jsdependenciesscrollingnavjs": "babel js\\dependencies\\scrolling-nav.js -o js\\dependencies\\scrolling-nav.js --require babel-polyfill --presets env",
        "compile-jsdmjs": "babel js\\dm.js -o js\\dm.js --require babel-polyfill --presets es2015-without-strict",
        "compile-jseventsjs": "babel js\\events.js -o js\\events.js --require babel-polyfill --presets es2015-without-strict",
        "compile-jshostjs": "babel js\\host.js -o js\\host.js --require babel-polyfill --presets es2015-without-strict",
        "compile-jshtml5js": "babel js\\html5.js -o js\\html5.js --require babel-polyfill --presets es2015-without-strict",
        "compile-jsnotifyjs": "babel js\\notify.js -o js\\notify.js --require babel-polyfill --presets es2015-without-strict",
        "compile-jsplayerjs": "babel js\\player.js -o js\\player.js --require babel-polyfill --presets es2015-without-strict",
        "compile-jssyncjs": "babel js\\sync.js -o js\\sync.js --require babel-polyfill --presets es2015-without-strict",
        "compile-jsvimeojs": "babel js\\vimeo.js -o js\\vimeo.js --require babel-polyfill --presets es2015-without-strict",
        "compile-jsytjs": "babel js\\yt.js -o js\\yt.js --require babel-polyfill --presets es2015-without-strict",
        "compile-playjs": "babel play.js -o play.js --require babel-polyfill --presets es2015-without-strict",
        "compile-serverjs": "babel server.js -o server.js --require babel-polyfill --presets es2015-without-strict",
        "compile-testjs": "babel test.js -o test.js --require babel-polyfill --presets es2015-without-strict",
        "compile-1_script": "npm run compile-apputilsupdatejs && npm run compile-assetsmainjs && npm run compile-jsdependenciesscrollingnavjs && npm run compile-jsdmjs && npm run compile-jseventsjs && npm run compile-jshostjs && npm run compile-jshtml5js && npm run compile-jsnotifyjs && npm run compile-jsplayerjs && npm run compile-jssyncjs && npm run compile-jsvimeojs && npm run compile-jsytjs && npm run compile-playjs && npm run compile-serverjs && npm run compile-testjs",
        "compile": "npm run compile-1_script"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/kyle8998/Vynchronize.git"
    },
    "author": "Kyle Lim",
    "license": "ISC",
    "dependencies": {
        "socket.io": "*",
        "express": "*"
    },
    "bugs": {
        "url": "https://github.com/kyle8998/Vynchronize/issues"
    },
    "homepage": "https://github.com/kyle8998/Vynchronize#readme",
    "devDependencies": {
        "jshint": "^2.9.5",
        "babel-cli": "latest",
        "babel-core": "latest",
        "babel-plugin-add-module-exports": "latest",
        "babel-plugin-transform-es2015-modules-commonjs": "latest",
        "babel-polyfill": "latest",
        "babel-preset-env": "latest",
        "babel-preset-es2015-without-strict": "latest"
    }
}
